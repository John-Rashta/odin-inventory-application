<html>
  <body>
    <ul>
    <h1><%= category.category_name %></h1>
    <% if (items) { %>
        <% items.map((item) => { %>
            <%- include("partials/groceryItem.ejs", {item}) %>
        <% }) %>
    <% } %>
    </ul>
    <a href=<%= `/categories/update/${category.id}` %> >Update</a>
    <form action=<%= `/categories/delete/${category.id}` %>  method="POST">
        <button type="submit">Delete</button>
    </form>
  </body>
</html>